# Use rofi to select an emoji
selected=$(echo "$emoji_list" | rofi -dmenu -i -p "Select Emoji" -theme ~/.config/rofi/config.rasi -no-custom)

# Check if an emoji was selected
if [ -n "$selected" ]; then
    # Extract just the emoji (first character/emoji)
    emoji=$(echo "$selected" | awk '{print $1}')
    
    # Copy to clipboard
    echo -n "$emoji" | wl-copy
    
    # Type the emoji using wtype (works better with Wayland)
    if command -v wtype &> /dev/null; then
        wtype "$emoji"
    else
        # Fallback: just copy to clipboard if wtype is not installed
        notify-send "Emoji Copied" "$emoji has been copied to clipboard" -t 2000
    fi
fi
